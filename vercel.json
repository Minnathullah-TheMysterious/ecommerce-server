{
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      },
      {
        "src": "build/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/v1/auth",
        "dest": "server.js"
      },
      {
        "src": "/api/v1/auth/(.*)",
        "dest": "server.js"
      },
      {
        "src": "/api/v1/category",
        "dest": "server.js"
      },
      {
        "src": "/api/v1/category/(.*)",
        "dest": "server.js"
      },
      {
        "src": "/api/v1/product",
        "dest": "server.js"
      },
      {
        "src": "/api/v1/product/(.*)",
        "dest": "server.js"
      },
      {
        "src": ".*",
        "dest": "server.js"
      }
    ]
  }